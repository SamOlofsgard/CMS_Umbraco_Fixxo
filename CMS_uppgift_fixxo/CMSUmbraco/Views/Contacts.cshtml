@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<ContentModels.Contacts>
@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;
@{
	Layout = "Layout.cshtml";
	ViewData["Title"] =@Model.Value("titleContacts");
}



<div id="contact">
        <div class="contact-start">
            <img src="images/products/Icon home.svg" alt="">
            Contacts
        </div>
        <div class="img-map">
            <img src="images/contact/contact-map.svg" alt="">
        </div>
        <form id="Contactform" onsubmit="">
            <div class="contact">@Model.Value("formHeader")</div>
            <div class="form-name">                
                <input type="text" id="contact-Name" placeholder="Your Name">
                <input type="email" id="contact-Email" placeholder="Your Email">
            </div> 
            <div class="form-message">
                <textarea type="textarea" id="contact-Message" placeholder="Comments" rows="4" form="messageForm"></textarea>
            </div>          
            <div>
                <button class="btn5" id="Contact-Submit" >Post Comments</button>
            </div>
        </form>
</div>  